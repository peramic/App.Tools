<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:w="urn:import:havis.custom.harting.tools.ui.client.widgets">
	<ui:with type="havis.custom.harting.tools.ui.resourcebundle.ConstantsResource" field="i18n" />
	<ui:with type="havis.custom.harting.tools.ui.resourcebundle.ResourceBundle" field="res" />
	<ui:style>

	</ui:style>
	<g:HTMLPanel styleName="{res.css.row}">
		<div class="{res.css.wideLabel}"><ui:text from="{i18n.operationTypeLabel}" /></div>
		<div class="{res.css.flexcol}"><w:CustomListBox ui:field="operationTypeList" styleName="{res.css.webuiListBox}"/></div>
	</g:HTMLPanel>
</ui:UiBinder> 