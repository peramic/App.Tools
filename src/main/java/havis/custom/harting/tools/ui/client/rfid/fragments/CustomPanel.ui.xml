<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:w="urn:import:havis.custom.harting.tools.ui.client.widgets">
	
	<ui:with type="havis.custom.harting.tools.ui.resourcebundle.ConstantsResource" field="i18n" />
	<ui:with type="havis.custom.harting.tools.ui.resourcebundle.ResourceBundle" field="res" />
	<g:HTMLPanel styleName="{res.css.row} {res.css.indented}">
		<div class="{res.css.customPanel}">
			<div class="{res.css.row}">
				<div class="{res.css.firstLabel}"><ui:text from="{i18n.bankLabel}"/></div>
				<w:CustomListBox ui:field="bankList" styleName="{res.css.webuiListBox}"/>
			</div>
			<div class="{res.css.row}">
				<div class="{res.css.firstLabel}"><ui:text from="{i18n.lengthLabel}"/></div>
				<g:IntegerBox ui:field="lengthIntegerBox" styleName="{res.css.webuiTextBox}"/>
			</div>
			<div class="{res.css.row}">
				<div class="{res.css.firstLabel}"><ui:text from="{i18n.offsetLabel}"/></div>
				<g:IntegerBox ui:field="offsetIntegerBox" styleName="{res.css.webuiTextBox}"/>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 