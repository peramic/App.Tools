<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="havis.custom.harting.tools.ui.resourcebundle.ResourceBundle" field="res"/>
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="{res.css.plusMinusBox} {res.css.webuiTextBox}">
		<g:Button styleName="{res.css.minusButton}" ui:field="minusButton"></g:Button>
		<g:TextBox ui:field="valueBox" styleName="{res.css.webuiTextBox}"></g:TextBox>
